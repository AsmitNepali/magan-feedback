{
    "name": "magan/feedback",
    "description": "This package helps to create quick contact-us form and send email to the admin.",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Asmit Nepali",
            "email": "mail2asmitnepali@email.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/magan/feedback",
    "keywords": ["Laravel", "Feedback"],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "~11"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Magan\\Feedback\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magan\\Feedback\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Magan\\Feedback\\FeedbackServiceProvider"
            ],
            "aliases": {
                "Feedback": "Magan\\Feedback\\Facades\\Feedback"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
